<h1>Lap Times for 
<%= @track.name %>
</h1> 

<table class="table">
  <thead>
    <tr>
      <th scope="col">Rank</th>
      <th scope="col">Vehicle</th>
      <th scope="col">Driver</th>
      <th scope="col">Time</th>
    </tr>
  </thead>
  <tbody>
  <% @tableData.each do |rowData| %>
    <tr>
      <td><%= rowData[:rank] %></td>
      <td><%= link_to  rowData[:vehicleName], user_vehicle_path(rowData[:userId], rowData[:vehicleId]) %></td>
      <td><%= link_to rowData[:driver], user_path(rowData[:userId]) %></td>
      <td><%= rowData[:lapTime] %></td>
    </tr>
  <% end %>
  </tbody>
    <br>
    <br>
 

 
