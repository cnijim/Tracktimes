<h1>Lap Times for 
<%= @track.name %>
</h1> 

<table class="table">
  <thead>
    <tr>
      <th scope="col">Rank</th>
      <th scope="col">Vehicle</th>
      <th scope="col">Driver</th>
      <th scope="col">Time</th>
    </tr>
  </thead>
  <tbody>
  <% @tableData.each do |rowData| %>
    <tr>
      <td><%= rowData[:rank] %></td>
      <td><%= rowData[:vehicleName] %></td>
      <td><%= rowData[:driver] %></td>
      <td><%= rowData[:lapTime] %></td>
    </tr>
  <% end %>
  </tbody>
    <br>
    <br>
 

 
